    <%
      function formatMoney(value) {
        if (value === null || value === undefined) return '—';
        const num = Number(value || 0);
        return num.toFixed(2);
      }
      function formatDelta(pct) {
        if (pct === null || pct === undefined) return '—';
        const sign = pct > 0 ? '+' : '';
        return `${sign}${(pct * 100).toFixed(1)}%`;
      }
      function formatDeltaUi(pct) {
        if (pct === null || pct === undefined) return 'N/A';
        const value = Math.abs(pct * 100).toFixed(1);
        if (pct > 0) return `▲ ${value}%`;
        if (pct < 0) return `▼ ${value}%`;
        return 'N/A';
      }
      function formatDeltaClass(pct) {
        if (pct === null || pct === undefined) return 'delta-na';
        if (pct > 0) return 'delta-pos';
        if (pct < 0) return 'delta-neg';
        return 'delta-na';
      }
      function formatPercent(value) {
        if (value === null || value === undefined) return '—';
        return `${(Number(value) * 100).toFixed(1)}%`;
      }
      function formatCount(value) {
        if (value === null || value === undefined) return '—';
        const num = Number(value || 0);
        return Number.isFinite(num) ? `${Math.trunc(num)}` : '—';
      }
