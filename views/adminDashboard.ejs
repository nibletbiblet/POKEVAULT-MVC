      exportBtns.forEach((btn) => {
        btn.addEventListener('click', () => {
          pendingExportUrl = btn.dataset.url || '';
          if (keyError) keyError.textContent = '';
          if (keyInput) keyInput.value = '';
          if (bootstrapModal) bootstrapModal.show();
        });
      });

      if (confirmBtn) {
        confirmBtn.addEventListener('click', () => {
          if (!pendingExportUrl) {
            if (keyError) keyError.textContent = 'No export target selected.';
            return;
          }
          const adminKey = keyInput ? keyInput.value.trim() : '';
          if (!adminKey) {
            if (keyError) keyError.textContent = 'Admin key is required.';
            return;
          }
          if (!confirm('Are you sure you want to proceed? This action cannot be undone.')) return;
          const baseUrl = pendingExportUrl;
          const sep = baseUrl.includes('?') ? '&' : '?';
          window.location.href = `${baseUrl}${sep}adminKey=${encodeURIComponent(adminKey)}`;
        });
      }
    })();
  </script>


