  <script>
    (function() {
      const messages = <%- JSON.stringify(success || []) %>;
      const container = document.getElementById('toastContainer');
      if (!container || !messages.length) return;
      function showToast(msg) {
        const toast = document.createElement('div');
        toast.className = 'toast-notification';
        toast.textContent = msg;
        container.appendChild(toast);
        setTimeout(() => toast.remove(), 3200);
      }
      messages.forEach(showToast);
    })();

    (function() {
      const ctx = document.getElementById('revenueChart');
      if (!ctx) return;
      const chartData = <%- JSON.stringify(stats.chart || {}) %>;
      const labels = chartData.labels || [];
      const metrics = chartData.current || {};
      const previous = chartData.previous || {};
      const groupByLabel = chartData.groupByLabel || 'Day';
      const metricLabels = {
        net_sales: 'Net Sales',
        orders: 'Orders',
        refunds: 'Refunds'
      };
      const metricFormatters = {
        net_sales: v => `$${Number(v || 0).toFixed(2)}`,
        refunds: v => `$${Number(v || 0).toFixed(2)}`,
        orders: v => `${Number(v || 0)}`
      };

      let activeMetric = 'net_sales';
      const buttons = Array.from(document.querySelectorAll('[data-metric]'));

      const buildDatasets = metric => ([
        {
          label: `${metricLabels[metric]} (Current)`,
          data: metrics[metric] || [],
          borderColor: 'rgba(125,243,212,0.85)',
          backgroundColor: 'rgba(125,243,212,0.18)',
          tension: 0.25,
          fill: true,
          pointRadius: 3,
          pointBackgroundColor: '#fff'
        },
        {
          label: `${metricLabels[metric]} (Previous)`,
          data: previous[metric] || [],
          borderColor: 'rgba(255,255,255,0.5)',
          backgroundColor: 'rgba(255,255,255,0)',
          tension: 0.25,
          fill: false,
          borderDash: [6, 4],
          pointRadius: 2,
          pointBackgroundColor: '#fff'
        }
      ]);

      const chart = new Chart(ctx, {
        type: 'line',
        data: {
          labels,
          datasets: buildDatasets(activeMetric)
        },
        options: {
          plugins: {
            legend: { display: false },
            tooltip: {
              callbacks: {
                title: items => {
                  const label = items && items.length ? items[0].label : '';
                  return `${groupByLabel}: ${label}`;
                },
                label: context => {
                  const formatter = metricFormatters[activeMetric] || (v => v);
                  const label = context.datasetIndex === 0 ? 'Current' : 'Previous';
                  const val = context.raw === null ? '—' : formatter(context.raw);
                  return `${label}: ${val}`;
                }
              }
            }
          },
          scales: {
            x: { ticks: { color: 'rgba(255,255,255,0.7)' }, grid: { display: false } },
            y: { ticks: { color: 'rgba(255,255,255,0.7)' }, grid: { color: 'rgba(255,255,255,0.08)' } }
          }
        }
      });

      buttons.forEach(btn => {
        btn.addEventListener('click', () => {
          const metric = btn.getAttribute('data-metric');
          if (!metric || metric === activeMetric) return;
          activeMetric = metric;
          buttons.forEach(b => b.classList.remove('active'));
          btn.classList.add('active');
          chart.data.datasets = buildDatasets(activeMetric);
          chart.update();
        });
      });

      ctx.onclick = (evt) => {
        if (groupByLabel !== 'Day') return;
        const points = chart.getElementsAtEventForMode(evt, 'nearest', { intersect: true }, true);
        if (!points.length) return;
        const idx = points[0].index;
        const label = labels[idx];
        if (!label) return;
        window.location.href = `/admin/reports/day?date=${encodeURIComponent(label)}`;
      };
    })();
  </script>
